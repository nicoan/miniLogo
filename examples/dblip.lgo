canvassize 500 500
backcolor 7


to dblip :length :iterations 
	ifelse :iterations=1 [forward :length] [ 
	call dblip :length :iterations-1 
	left 90 
	call dblip :length :iterations-1 
	call dblip :length :iterations-1 
	right 90 
	call dblip :length :iterations-1 
	right 90 
	call dblip :length :iterations-1 
	call dblip :length :iterations-1 
	left 90 
	call dblip :length :iterations-1 
	] 
end


call dblip 10 7